<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fourgroup.dao.ElectronicDao">

    <select id="findEle" resultType="ElectronicReceipt" parameterType="ElectronicReceipt">
        select * from electronic where EleId=#{eletronicId}
    </select>
    <select id="findAcc" resultType="Account" parameterType="String">
        select * from account where accountid=#{account}
    </select>
</mapper>

